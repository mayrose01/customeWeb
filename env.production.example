# 生产环境配置示例文件
# 复制为 .env.production 并修改相应值

# 数据库配置
MYSQL_ROOT_PASSWORD=your_strong_mysql_root_password
MYSQL_DATABASE=enterprise_prod
MYSQL_USER=enterprise_user
MYSQL_PASSWORD=your_strong_mysql_password
DATABASE_URL=mysql+pymysql://enterprise_user:your_strong_mysql_password@mysql:3306/enterprise_prod

# JWT配置
SECRET_KEY=your_very_strong_secret_key_2024_production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# CORS配置
CORS_ORIGINS=["https://catusfoto.top", "https://www.catusfoto.top"]

# 前端配置
VITE_API_BASE_URL=https://catusfoto.top/api
VITE_APP_TITLE=企业官网

# 邮件配置
SMTP_SERVER=smtp.gmail.com
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# 性能配置
WORKERS=8
